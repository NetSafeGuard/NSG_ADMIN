{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "NetSafeGuard Admin",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "process": {
        "all": false,
        "exit": false,
        "relaunch": true,
        "relaunchDangerousAllowSymlinkMacos": false
      },
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": false,
        "close": true,
        "hide": true,
        "show": true,
        "maximize": true,
        "minimize": true,
        "unmaximize": true,
        "unminimize": true,
        "startDragging": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "shortDescription": "Admin NetSafeGuard",
      "identifier": "admin.netsafeguard.developers",
      "icon": [
        "icons/32x32.png",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDEzMUVBOEI2RDBFOTE2QkIKUldTN0Z1blF0cWdlRTlqeDhDZjJhUmx5eDRkSUNvL3JUM1ZDcStPUFptWVAza1ZGQlVieDVsSEIK",
      "endpoints": [
        "https://api.netsafeguard.cloud/api/v1/update?current_version={{current_version}}&target={{target}}&arch={{arch}}"
      ],
      "windows": {
        "installMode": "basicUi"
      }
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Admin NetSafeGuard",
        "width": 1090,
        "height": 624,
        "center": true,
        "minWidth": 640,
        "minHeight": 480,
        "decorations": false,
        "transparent": true
      }
    ]
  }
}
